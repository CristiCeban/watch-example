{
  "expo": {
    "name": "watch-example",
    "slug": "watch-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "react-native-watch-connectivity",
        {
          "devTeamId": "39L46PD99U",
          "targets": [
            {
              "type": "watch",
              "name": "watch",
              "bundleId": "com.mpiannucci.example1.watch",
              "displayName": "EAS WATCH",
              "companionAppBundleId": "com.mpiannucci.example1",
              "sourceDir": "watch",
              "sourceFiles": ["watchApp.swift"],
              "entitlementsFile": "watch.entitlements",
              "frameworks": ["SwiftUI.framework"]
            }
          ]
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expo.watch.example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
